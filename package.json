    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "16.3.2",
    "nx-cloud": "latest",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "~5.0.2",
    "verdaccio": "^5.0.4"
  }
